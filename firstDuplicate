var firstDuplicate;

function firstDuplicate(array){ 
    
    for(var i = 0; i < array.length; i++) {
        if (set.contains(array[i])) {
            return array[i];
        }
        set.add(array[i]);
    }
    return -1;
}

var a = [2, 1, 3, 5, 3, 2];
firstDuplicate(array);

var a = [2, 4, 3, 5, 1];
firstDuplicate(array);
